{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "member-access": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "double"
    ],
    "arrow-parens": false,
    "semicolon": false,
    "no-console": [
      true,
      "warning"
    ],
    "no-trailing-whitespace": false,
    "prefer-const": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "max-line-length": false,
    "trailing-comma": false,
    "eofline": false,
    "typedef-whitespace": false,
    "no-var-requires": false,
    "prefer-for-of": false,
    "forin": false,
    "variable-name": false,
    "no-empty": false,
    "noImplicitThis": false,
    "no-shadow": false,
    "no-shadowed-variable": false,
    "only-arrow-functions": false
  }
}